<h1>Save movies</h1>

<h3>My Lists</h3>
<div class="row">
    <% @lists.each do |list| %>
      <div class="co-12 col-sm-6 col-md-4">
        <%= link_to list_path(list), class: "text-decoration-none" do %>
            <%= list.name %>
          </div>
        <% end %>
      </div>
    <% end %>
  </div>
</div>

<%= link_to "Create a movie list", new_list_path, class: "btn btn-primary" %>
