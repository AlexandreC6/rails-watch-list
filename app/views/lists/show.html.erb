<h2><%= @list.name %></h2>

<ul class="list-group">
  <% @list.bookmarks.each do |bookmark| %>
    <li class="list-group-item"><%= bookmark.content %></li>
    <li class="list-group-item"><%= bookmark.movie.title %></li>
  <% end %>
</ul>

<%= link_to "Back to the list", lists_path %>
<%= link_to "Create a bookmark", new_list_bookmark_path(@list)%>
